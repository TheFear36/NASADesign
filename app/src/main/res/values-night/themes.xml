<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="BaseTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">?myColor_200</item>
        <item name="colorOnPrimary">?myColor_800</item>
        <item name="colorPrimaryContainer">?myColor_700</item>
        <item name="colorOnPrimaryContainer">?myColor_100</item>
        <item name="colorPrimaryInverse">?myColor_600</item>
        <item name="colorSecondary">?mySecondColor_200</item>
        <item name="colorOnSecondary">?mySecondColor_800</item>
        <item name="colorSecondaryContainer">?mySecondColor_700</item>
        <item name="colorOnSecondaryContainer">?mySecondColor_100</item>
        <item name="colorTertiary">?myThirdColor_200</item>
        <item name="colorOnTertiary">?myThirdColor_800</item>
        <item name="colorTertiaryContainer">?myThirdColor_700</item>
        <item name="colorOnTertiaryContainer">?myThirdColor_100</item>
        <item name="colorSurface">?myColor_600</item>

<!--        <item name="floatingActionButtonStyle">@style/myFloatingActionButtonStyleNight</item>-->
        <item name="textInputStyle">@style/myTextInputStyle</item>

        <item name="myColor_100">@color/yellow_100</item>
        <item name="myColor_200">@color/yellow_200</item>
        <item name="myColor_600">@color/yellow_600</item>
        <item name="myColor_700">@color/yellow_700</item>
        <item name="myColor_800">@color/yellow_800</item>

        <item name="mySecondColor_100">@color/purple_100</item>
        <item name="mySecondColor_200">@color/purple_200</item>
        <item name="mySecondColor_700">@color/purple_700</item>
        <item name="mySecondColor_800">@color/purple_800</item>

        <item name="myThirdColor_100">@color/indigo_100</item>
        <item name="myThirdColor_200">@color/indigo_200</item>
        <item name="myThirdColor_700">@color/indigo_700</item>
        <item name="myThirdColor_800">@color/indigo_800</item>

    </style>
</resources>